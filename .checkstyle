<?xml version="1.0" encoding="UTF-8"?>
<fileset-config file-format-version="1.2.0" simple-config="true" sync-formatter="false">
  <local-check-config name="tcss305" location="tcss305_checkstyle_sameline.xml" type="project" description="">
    <additional-data name="protect-config-file" value="false"/>
  </local-check-config>
  <fileset name="all" enabled="true" check-config-name="tcss305" local="true">
    <file-match-pattern match-pattern="." include-pattern="true"/>
    <!-- Add exclusion patterns for non-Java files -->
    <file-match-pattern match-pattern="\.db$" include-pattern="false"/>
    <file-match-pattern match-pattern="\.jar$" include-pattern="false"/>
    <file-match-pattern match-pattern="\.png$" include-pattern="false"/>
    <file-match-pattern match-pattern="\.jpg$" include-pattern="false"/>
    <file-match-pattern match-pattern="\.wav$" include-pattern="false"/>
    <file-match-pattern match-pattern="\.ttf$" include-pattern="false"/>
    <file-match-pattern match-pattern="resources/" include-pattern="false"/>
  </fileset>
</fileset-config>
